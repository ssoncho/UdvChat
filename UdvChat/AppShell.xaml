<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="UdvChat.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:pages="clr-namespace:UdvChat.Presentation.Pages"
    xmlns:str="clr-namespace:UdvChat.Resources.Strings"
    Shell.FlyoutBehavior="Disabled"
    Title="UdvChat">

    <ShellContent
        Title="{x:Static str:Strings.AppName}"
        ContentTemplate="{DataTemplate pages:ChatListPage}"
        Route="MainPage" />

</Shell>
